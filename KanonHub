local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/NeyuDev/Kanon-Hub/main/OrionLib')))()
local Window = OrionLib:MakeWindow({Name = "Kanon Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
    Name = "BladeBall",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
OrionLib:MakeNotification({
    Name = "Kanon Hub",
    Content = "Đây chỉ là bản thử nghiệm, thêm chức năng lấy key sau",
    Image = "rbxassetid://4483345998",
    Time = 10
})
Tab:AddButton({
    Name = "Auto Chặn Bóng",
    Callback = function()
              print("button pressed")
            loadstring(game:HttpGet("https://scriptblox.com/raw/UPD-Blade-Ball-op-autoparry-with-visualizer-8652"))()
      end    
})

Tab:AddDropdown({
    Name = "Social",
    Default = "1",
    Options = {".gg/7nQHQDEj", "2"},
    Callback = function(Value)
        print(Value)
    end    
})

local Tab = Window:MakeTab({
    Name = "Đang Thử Nghiệm",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = true
})

local Tab = Window:MakeTab({
    Name = "Credit",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Tab:AddParagraph("Lưu Ý","Đây chỉ là bản thử nghiệm còn sẽ được cập nhật và UI này được sử dụng free bởi Orion Library. Discord: unineyu")
OrionLib:Init()

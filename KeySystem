

local Window = Rayfield:CreateWindow({
   Name = "KEY SYSTEM",
   LoadingTitle = "Load Key System",
   LoadingSubtitle = "by UniNeyu",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "keylist"
   },
   Discord = {
      Enabled = true,
      Invite = "Hhrpe3X559", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = false -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Kanon Hub | Key",
      Subtitle = "Key System",
      Note = "by UniNeyu",
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = false, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = true, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"https://raw.githubusercontent.com/NeyuDev/Kanon-Hub/main/keylist"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

loadstring(game:HttpGet("https://raw.githubusercontent.com/NeyuDev/Kanon-Hub/main/KanonHub"))()

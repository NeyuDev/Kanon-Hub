local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

function createKeyPanel()
    local Window = Rayfield:CreateWindow({
        Name = "Key",
        Title = "Enter Key",
        Subtitle = "For Kanon Hub",
    })

    local Tab = Window:CreateTab("Key", 4483362458)
    local Input = Tab:CreateInput({
        Name = "Key:",
        SetPlaceholder("Enter Key"),
        SetRequired(true),
    })

    local Button = Tab:CreateButton({
        Name = "Create Key",
        Callback = function()
            local key = createKey()
            Input:SetValue(key)
        end,
    })

    local Button = Tab:CreateButton({
        Name = "Save Key",
        Callback = function()
            if Input:GetValue() ~= nil then
                saveKey(Input:GetValue())
            else
                print("Key is required!")
            end
        end,
    })

    local Button = Tab:CreateButton({
        Name = "Check Key",
        Callback = function()
            local loadedKey = loadstring(game:HttpGet('https://raw.githubusercontent.com/NeyuDev/Kanon-Hub/main/keylist'))()
            if Input:GetValue() == loadedKey then
                Window:Destroy()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/your/script/main/script.lua"))()
            else
                print("Invalid key!")
            end
        end,
    })

    return Window
end

createKeyPanel()
